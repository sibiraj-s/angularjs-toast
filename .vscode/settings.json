{
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".sass-cache": true,
    "node_modules/": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true
}
